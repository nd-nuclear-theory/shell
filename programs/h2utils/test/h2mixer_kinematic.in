# h2mixer_kinematic.in -- test of kinematic operator generation

set-target-indexing-oscillator tb 6
set-target-multipolarity 0 0 0
set-output-format 0
set-mass 2

define-radial-operator r 1 1 1 0 test/ob-rme-ho-r1-Nmax20.dat
define-radial-operator r 2 0 0 0 test/ob-rme-ho-r2-Nmax20.dat
define-radial-operator k 1 1 1 0 test/ob-rme-ho-k1-Nmax20.dat
define-radial-operator k 2 0 0 0 test/ob-rme-ho-k2-Nmax20.dat

define-source operator Ursqr
define-source operator Vr1r2
define-source operator Uksqr
define-source operator Vk1k2

define-target test/tbme-rsqr-tb-6-v0.dat
  add-source Ursqr 1
define-target test/tbme-r1r2-tb-6-v0.dat
  add-source Vr1r2 1
define-target test/tbme-ksqr-tb-6-v0.dat
  add-source Uksqr 1
define-target test/tbme-k1k2-tb-6-v0.dat
  add-source Vk1k2 1

