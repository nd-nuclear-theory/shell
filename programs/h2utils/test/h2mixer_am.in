# h2mixer_am.in -- reproduce angular momentum operator TBMEs (A=2)
# from tbme-am-sqr-150620

set-target-oscillator ob 1
set-target-multipolarity 0 0 0
set-output-format 0
set-mass 2

define-operator-source Lp
define-target Lp test/tbme-Lp.dat
  add-source Lp 1

define-operator-source Ln
define-target Ln test/tbme-Ln.dat
  add-source Ln 1

define-operator-source L
define-target L test/tbme-L.dat
  add-source L 1

define-operator-source Sp
define-target Sp test/tbme-Sp.dat
  add-source Sp 1

define-operator-source Sn
define-target Sn test/tbme-Sn.dat
  add-source Sn 1

define-operator-source S
define-target S test/tbme-S.dat
  add-source S 1

define-operator-source Jp
define-target Jp test/tbme-Jp.dat
  add-source Jp 1

define-operator-source Jn
define-target Jn test/tbme-Jn.dat
  add-source Jn 1

define-operator-source J
define-target J test/tbme-J.dat
  add-source J 1
