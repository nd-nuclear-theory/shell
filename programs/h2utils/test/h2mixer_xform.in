define-xform-source identity-1.414 test/tbme-identity-tb-2-v0.dat 2 2 2 2 2 test/radial-olap-ho-b1.414-Nmax20.dat

define-target identity-1.000 test/tbme-xform.dat
  add-source identity-1.000 1

define-target identity-1.414 test/tbme-xform.dat
  add-source identity-1.414 1