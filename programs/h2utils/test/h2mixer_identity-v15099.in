# h2mixer_identity-v*****.in -- test of identity operator generation for given file format

set-target-indexing-oscillator tb 2
set-target-multipolarity 0 0 0
set-output-format 15099
set-mass 2

define-source operator identity

define-target test/tbme-identity-tb-2-v15099.dat
  add-source identity 1

define-target test/tbme-identity-tb-2-v15099.bin
  add-source identity 1